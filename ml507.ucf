NET sys_clk_pin LOC= AH15 | IOSTANDARD = LVCMOS33;
NET sys_clk_pin TNM_NET = sys_clk_pin;

TIMESPEC TS_sys_clk_pin = PERIOD sys_clk_pin 10000 ps;


NET clocks/rst TIG;
#NET clocks/nuke_i TIG;

# Ethernet PHY

TIMEGRP "gmii_tx"=PADS("gmii_tx*");
TIMEGRP "gmii_tx" OFFSET = OUT AFTER sys_clk_pin REFERENCE_PIN "gmii_tx_clk" RISING;


NET phy_rstb LOC=J14 | IOSTANDARD=LVCMOS25;

NET gmii_tx_clk LOC=J16 | IOSTANDARD=LVCMOS25 | SLEW="FAST";
NET gmii_txd<0> LOC=AF11 | IOSTANDARD=LVCMOS33;
