NET gmii_txd<7> LOC=AG11 | IOSTANDARD=LVCMOS33;
NET gmii_tx_en LOC=AJ10 | IOSTANDARD=LVCMOS33;
NET gmii_tx_er LOC=AJ9 | IOSTANDARD=LVCMOS33;

NET gmii_rx_clk LOC=H17 | IOSTANDARD=LVCMOS25 | TNM= "gmii_rx_clk";
TIMESPEC "TS_GMII_RX_CLK" = PERIOD "gmii_rx_clk" 125MHz;
OFFSET = IN 2.5ns VALID 3ns BEFORE gmii_rx_clk;

